PROMPT Creating Table 'DIPLOME'
CREATE TABLE DIPLOME
 (NO_ETUDIANT_NAT VARCHAR2(50) NOT NULL
 ,NOM VARCHAR2(50) NOT NULL
 ,PRENOM VARCHAR2(50) NOT NULL
 ,AUTORISATION_ANNUAIRE CHAR(1) DEFAULT 'N' NOT NULL
 ,TYPE_EMPLOI VARCHAR2(3)
 ,MAIL_PRO VARCHAR2(100)
 ,TEL_PRO VARCHAR2(20)
 ,ADRESSE VARCHAR2(100)
 ,CP VARCHAR2(10)
 ,VILLE VARCHAR2(50)
 ,EMAIL_PERSO VARCHAR2(100)
 ,TEL_PERSO VARCHAR2(20)
 ,PORTABLE VARCHAR2(20)
 )
/

COMMENT ON COLUMN DIPLOME.NO_ETUDIANT_NAT IS 'Numéro National de l''étudiant'
